Algoritmo "problema_fatorial"

Var
 n, i : inteiro
 
 f : real

Inicio

   escreval ("Digite o valor de N: ")
   leia (n)
   
   f <- 1

    para i de 1 ate n faca
     f <- f * i

fimpara

     escreval ("FATORIAL =", f)
     
     //valor fatorial é a multplicão do numero em ordem decrescente


Fimalgoritmo